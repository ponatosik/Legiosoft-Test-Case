{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ExcelTransactionExport": {
      "IdHeaderText": "Transaction",
      "NameHeaderText": "Name",
      "EmailHeaderText": "Email",
      "AmountHeaderText": "Amount",
      "ClientLocationHeaderText": "Location",
      "UtcTimeHeaderText": "UTC time",
      "WorksheetName": "Transactions",
      "AutoSizeColumns": true
  },
  "CsvTransactionImport": {
      "IdFieldName": "transaction_id",
      "NameFieldName": "name",
      "EmailFieldName": "email",
      "AmountFieldName": "amount",
      "ClientLocationFieldName": "client_location",
      "LocalTimeFieldName": "transaction_date",
      "AmountFieldIncludesCurrency": true,
      "AmmountFieldPossibleCurrencySigns": [ "$" ]
  }
}
